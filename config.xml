<config>
    <dsg>
        <port>/dev/ttyAMA2</port>
        <baudrate>4800</baudrate>
        <parity>PARITY_NONE</parity>
        <stopbits>STOPBITS_ONE</stopbits>
        <bytesize>EIGHTBITS</bytesize>
        <timeout>1</timeout>
    </dsg>
    <drrg>
        <port>/dev/ttyAMA3</port>
        <baudrate>9600</baudrate>
        <parity>PARITY_EVEN</parity>
        <stopbits>STOPBITS_ONE</stopbits>
        <bytesize>EIGHTBITS</bytesize>
        <timeout>1</timeout>
    </drrg>
    <lora>
        <port>/dev/ttyAMA4</port>
        <baudrate>9600</baudrate>
        <parity>PARITY_NONE</parity>
        <stopbits>STOPBITS_ONE</stopbits>
        <bytesize>EIGHTBITS</bytesize>
        <timeout>2</timeout>
    </lora>
    <datalogpath>/home/postekit/POSTe/data_log.csv</datalogpath>
    <eventlogpath>/home/postekit/POSTe/event_log.csv</eventlogpath>
    <cmsginterval>60</cmsginterval>
</config>